<?xml version='1.0' encoding='UTF-16'?><Properties version='1.1'><Common><Context type='int'>1</Context><Variant type='string'>12c</Variant><DescriptorVersion type='string'>1.0</DescriptorVersion><PartitionType type='int'>-1</PartitionType><RCP type='int'>0</RCP></Common><Connection><Server type='string'><![CDATA[#PBM_BNP_GLOBAL.$pTGT_DB#]]></Server><Username type='string'><![CDATA[#PBM_BNP_GLOBAL.$pTGT_DB_USER#]]></Username><Password type='string'><![CDATA[#PBM_BNP_GLOBAL.$pTGT_DB_PWD#]]></Password><OSLevelAuthentication type='bool'><![CDATA[0]]></OSLevelAuthentication><Version modified='1' type='string'><![CDATA[12c]]></Version></Connection><Usage><ReadMode type='int'><![CDATA[0]]></ReadMode><GenerateSQL type='bool'><![CDATA[0]]></GenerateSQL><EnableQuotedIDs type='bool'><![CDATA[0]]></EnableQuotedIDs><SQL><SelectStatement modified='1' type='string'><![CDATA[SELECT IH.INVOICE_NBR,
       TO_CHAR (IH.INVOICE_DT, 'YYYYMMDD') AS INVOICE_DT,
       IH.BILLING_ENTITY_ID,
       IH.BILLING_ENTITY_TYPE_CD,
       VBA.BILLING_ACCOUNT_NAME,
       VBE.BILLING_TYPE_CD
  FROM    #PBM_BNP_DAILY.pTABLE_NAME_BP_INVOICE_HEADER# IH
       LEFT OUTER JOIN
          #PBM_BNP_DAILY.pVIEW_NAME_V_BILLING_ACCOUNT# VBA 
       ON IH.BILLING_ENTITY_ID = VBA.BILLING_ACCOUNT_ID
          LEFT OUTER JOIN #PBM_BNP_DAILY.pVIEW_NAME_V_BILLING_ENTITY# VBE
       ON IH.BILLING_ENTITY_ID = VBE.BILLING_ENTITY_ID
 WHERE     1=1
